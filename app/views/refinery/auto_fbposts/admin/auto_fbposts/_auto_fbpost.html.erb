<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(auto_fbpost) -%>">
  <span class='title'>
    <%= auto_fbpost. %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_auto_fbposts_admin_auto_fbpost_path(auto_fbpost),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.auto_fbposts_admin_auto_fbpost_path(auto_fbpost),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => auto_fbpost.),
        :method => :delete %>
  </span>
</li>
